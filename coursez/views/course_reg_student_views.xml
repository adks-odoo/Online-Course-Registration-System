<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="course_reg_student_action" model="ir.actions.act_window">
        <field name="name">Students Details</field>
        <field name="res_model">course.reg.student</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No Record Found. Let's create one!!
            </p>
        </field>
    </record>
    <record id="course_reg_student_view_tree" model="ir.ui.view">
        <field name="name">course.reg.student.tree</field>
        <field name="model">course.reg.student</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="name" />
                <field name="student_id" />
                <field name="email" />
                <field name="contact_no" />
                <field name="grade_level" />
                <field name="registered_courses" />
            </tree>
        </field>
    </record>
</odoo>
